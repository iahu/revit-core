{
  "name": "@egret-revit/core",
  "version": "0.2.6",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && npm run build:esm",
    "build:esm": "rollup --config rollup.config.js",
    "serve": "vite preview",
    "watch": "yarn build:esm --watch",
    "test": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint --fix --config .eslintrc.json",
    "precommit": "echo 'precommit' && lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "main": "./dist/core.umd.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/core.umd.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/fabric": "^4.5.3",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.1",
    "rollup": "^2.60.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "typescript": "^4.4.4",
    "vite": "^2.4.3"
  },
  "dependencies": {
    "@dxfom/dxf": "^0.0.2",
    "konva": "^8.2.3",
    "rxjs": "~7.3.0"
  }
}
